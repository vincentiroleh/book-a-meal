{
  "name": "meal-booking-api",
  "version": "1.0.0",
  "description": "a meal booking api",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/index.js",
    "build": "babel -d ./build ./api -s",
    "dev": "nodemon --exec babel-node ./api/index.js",
    "app": "nodemon --exec babel-node ./server/app.js",
    "test": "mocha --require @babel/register --exit || true",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vincentiroleh/book-a-meal.git"
  },
  "keywords": [
    "meals",
    "booking",
    "food",
    "chop"
  ],
  "author": "Vincent Iroleh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vincentiroleh/book-a-meal/issues"
  },
  "homepage": "https://github.com/vincentiroleh/book-a-meal#readme",
  "dependencies": {
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "expect": "^24.5.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "pg": "^7.8.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.43.0",
    "sequelize-cli": "^5.4.0",
    "supertest": "^4.0.0",
    "uuid": "^3.3.2",
    "validator": "^10.11.0",
    "volleyball": "^1.5.1"
  },
  "engines": {
    "node": "8.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0"
  },
  "directories": {
    "doc": "docs"
  }
}
